    "developer.mozilla.org": {
      "crawl_frequency_limit": "48h",
      "snippet_tag_filter": ["article", "main", "p", "pre", "code"],
      "language_whitelist": ["en"],
      "enabled": true,
      "priority_multiplier": 1.4
