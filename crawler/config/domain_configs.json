{
  "domains": {
    "news.ycombinator.com": {
      "crawl_frequency_limit": "1h",
      "snippet_tag_filter": ["div.comment", "p"],
      "language_whitelist": ["en"],
      "enabled": true,
      "priority_multiplier": 1.4
    },
    "reddit.com": {
      "crawl_frequency_limit": "2h",
      "snippet_tag_filter": ["div.comment", "div.post-body", "p"],
      "language_whitelist": ["en"],
      "enabled": true,
      "priority_multiplier": 0.9
    },
    "reuters.com": {
      "crawl_frequency_limit": "2h",
      "snippet_tag_filter": ["p", "article", "div.article-body"],
      "language_whitelist": ["en"],
      "enabled": true,
      "priority_multiplier": 1.3
    },
    "techcrunch.com": {
      "crawl_frequency_limit": "3h",
      "snippet_tag_filter": ["div.article-content", "p"],
      "language_whitelist": ["en"],
      "enabled": true,
      "priority_multiplier": 1.1
    },

    "wikipedia.org": {
      "crawl_frequency_limit": "24h",
      "snippet_tag_filter": ["p", "div.mw-parser-output", "main"],
      "language_whitelist": ["en"],
      "enabled": true,
      "priority_multiplier": 1.2
