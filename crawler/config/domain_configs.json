      "priority_multiplier": 1.4
    },
    "example-disabled.com": {
      "crawl_frequency_limit": "24h",
      "snippet_tag_filter": [],
      "language_whitelist": [],
      "enabled": false,
      "priority_multiplier": 0.0
    }
